{
  "name": "alert-alert-demo",
  "version": "1.0.0",
  "description": "Demo for the alert-alert node package",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf demo && mkdir demo",
